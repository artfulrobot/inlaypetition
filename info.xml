<?xml version="1.0"?>
<extension key="inlaypetition" type="module">
  <file>inlaypetition</file>
  <name>Petition Inlay</name>
  <description>Provides embeddable petitions for remote websites. Built using the Inlay API.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Rich Lott / Artful Robot</author>
    <email>forums@artfulrobot.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/artfulrobot/inlaypetition</url>
    <url desc="Documentation">https://github.com/artfulrobot/inlaypetition</url>
    <url desc="Support">https://github.com/artfulrobot/inlaypetition</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-11-10</releaseDate>
  <version>1.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.31</ver>
  </compatibility>
  <comments>This is new and WIP. It may be useful to you as-is, or as a starting point for your own extension.</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Inlaypetition</namespace>
  </civix>
</extension>
